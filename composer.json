{
  "name": "bitmotion/mwimagemap",
  "type": "typo3-cms-extension",
  "description": "forked mwimagemap extension",
  "keywords": [
    "TYPO3",
    "extension",
    "article",
    "news"
  ],
  "homepage": "http://bitmotion.de",
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": "^7.6.13 || >=8.5.0 <9"
  },
  "suggest": {
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "nimut/testing-framework": "^1.0"
  },
  "autoload": {
    "files": [
      "pi1/class.tx_mwimagemap_pi1.php",
      "constants.php"
    ]
  },
  "replace": {
    "mwimagemap": "self.version",
    "typo3-ter/mwimagemap": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
